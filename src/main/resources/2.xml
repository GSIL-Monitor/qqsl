<!--水力要素-->
<waterResources>
    <!--底流式消力池-->
    <attributeGroup name="水力要素" buildAlias="b0001" alias="wr" >
        <attribute name='跃后水深h"c' unit="m" type="number" alias="wr0001"/>
        <attribute name="下游水深ht" unit="m" type="number" alias="wr0002"/>
        <attribute name="水跃淹没系数σ" unit="1.05～1.1" type="number" alias="wr0003"/>
        <attribute name="池出口水面落差△z" unit="m" type="number" alias="wr0004"/>
        <attribute name="水跃长度Lj" unit="m" type="number" alias="wr0005"/>
        <attribute name="尾坎顶水头" unit="m" type="number" alias="wr0006"/>
    </attributeGroup>
    <!--谷坊-->
    <attributeGroup name="水力要素" buildAlias="b0002" alias="wr" >
        <attribute name="设计洪水流量Q" unit="m3/s" type="number" alias="wr0007"/>
        <attribute name="堰上临界水深hc" unit="m" type="number" alias="wr0008"/>
    </attributeGroup>
    <!--开敞式水闸-->
    <attributeGroup name="水力要素" buildAlias="b0056" alias="wr" >
        <attribute name="过闸总流量Q" unit="m3/s" type="number" alias="wr0009"/>
        <attribute name="堰流流量系数m" unit="" type="number" alias="wr0010"/>
        <attribute name="侧收缩系数ε" unit="" type="number" alias="wr0011"/>
        <attribute name="淹没系数σs" unit="" type="number" alias="wr0012"/>
    </attributeGroup>
    <!--圆形蓄水池-->
    <attributeGroup name="水力要素" buildAlias="b0060" alias="wr" >
        <attribute name="容积" unit="m³" type="select" select="50,100,150,200,300,400,500,600,800,1000,1500,2000" alias="wr00013"/>
    </attributeGroup>
    <!--泉室-->
    <attributeGroup name="水力要素" buildAlias="b0003" alias="wr" >
        <attribute name="容积" unit="m³" type="select" select="5,10,20,30,40,50" alias="wr00014"/>
    </attributeGroup>
    <!--渡槽-->
    <attributeGroup name="水力要素" buildAlias="b0061" alias="wr" >
        <attribute name="设计流量Q" unit="m3/s" type="number" alias="wr00015"/>
    </attributeGroup>
    <!--大口井-->
    <attributeGroup name="水力要素" buildAlias="b0005" alias="wr" >
        <attribute name="设计流量Q" unit="m3/h" type="number" alias="wr00016"/>
        <attribute name="最低动水位" unit="m" type="number" alias="wr00017"/>
    </attributeGroup>
    <!--管井-->
    <attributeGroup name="水力要素" buildAlias="b0062" alias="wr" >
        <attribute name="设计流量Q" unit="m3/h" type="number" alias="wr00018"/>
        <attribute name="最低动水位" unit="m" type="number" alias="wr00019"/>
    </attributeGroup>
    <!--海漫-->
    <attributeGroup name="水力要素" buildAlias="b0014" alias="wr" >
        <attribute name="上下游水头差△Ｈ" unit="m" type="number" alias="wr00020"/>
        <attribute name="消力池末端单宽流量ｑｓ" unit="m3/s" type="number" alias="wr00021"/>
        <attribute name="长度系数Ｋｓ" unit="14～7" type="number" alias="wr00022"/>
    </attributeGroup>
    <!--铺盖-->
    <attributeGroup name="水力要素" buildAlias="b0063" alias="wr" >
        <attribute name="上下游水头差△Ｈ" unit="m" type="number" alias="wr00023"/>
    </attributeGroup>
    <!--跌水-->
    <attributeGroup name="水力要素" buildAlias="b0011" alias="wr" >
        <attribute name="设计流量Q" unit="m3/s" type="number" alias="wr0024"/>
        <attribute name="流量系数m" unit="" type="number" alias="wr0025"/>
        <attribute name="侧收缩系数ε" unit="0.85～0.95" type="number" alias="wr0026"/>
        <attribute name="水跃长度Lj" unit="m" type="number" alias="wr0027"/>
        <attribute name="水舌射流长度L1" unit="m" type="number" alias="wr0028"/>
    </attributeGroup>
    <!--水表井-->
    <attributeGroup name="水力要素" buildAlias="b0064" alias="wr" >
        <attribute name="设计流量Q" unit="m3/s" type="number" alias="wr0029"/>
    </attributeGroup>
    <!--曲线式倒虹吸-->
    <attributeGroup name="水力要素" buildAlias="b0065" alias="wr" >
        <attribute name="设计流量Q" unit="m3/s" type="number" alias="wr0030"/>
    </attributeGroup>
    <!--陡坡-->
    <attributeGroup name="水力要素" buildAlias="b0066" alias="wr" >
        <attribute name="设计流量Q" unit="m3/s" type="number" alias="wr0031"/>
    </attributeGroup>
</waterResources>