        <attribe name="描述" unit="建筑物名称或标记" type="text" alias="remark"/>
    </attribeGroup>
    <!--开敞式水闸-->
    <attribeGroup name="坐标" buildAlias="b0054" alias="coor">
        <attribe name="中心点（闸底板上游顶边中心点）" unit="经度,纬度,高程" type="text" alias="center"/>
