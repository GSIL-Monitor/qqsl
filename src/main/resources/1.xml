<!--坐标-->
<coordinate>
    <!--底流式消力池-->
    <attributeGroup name="坐标" buildAlias="b0001" alias="coor">
        <attribute name="中心点" unit="经度,纬度" type="text" alias="center"/>
        <attribute name="定位点" unit="经度,纬度" type="text" alias="position"/>
        <attribute name="设计标高" unit="消力池底板顶（m）" type="number" alias="designElevation"/>
        <attribute name="描述" unit="建筑物名称或标记" type="text" alias="remark"/>
    </attributeGroup>
    <!--谷坊-->
