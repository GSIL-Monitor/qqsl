<!--坐标-->
<coordinate>
    <!--底流式消力池-->
    <attribeGroup name="坐标" buildAlias="b0001" alias="coor">
        <attribe name="中心点" unit="经度,纬度,高程" type="text" alias="center"/>
        <attribe name="定位点" unit="经度,纬度,高程" type="text" alias="position"/>
        <attribe name="设计标高" unit="消力池底板顶（m）" type="number" alias="designElevation"/>
        <attribe name="描述" unit="建筑物名称或标记" type="text" alias="remark"/>
    </attribeGroup>
    <!--谷坊-->
    <attribeGroup name="坐标" buildAlias="b0002" alias="coor">
        <attribe name="中心点" unit="经度,纬度,高程" type="text" alias="center"/>
